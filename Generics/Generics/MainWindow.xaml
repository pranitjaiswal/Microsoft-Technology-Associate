<Window x:Class="Generics.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded_1">
    <StackPanel>
        <TextBlock Text="Collection binding demo" Margin="3"></TextBlock>
        <TextBlock Text="All faculty name" Margin="3"></TextBlock>
        <ListBox ItemsSource="{Binding}" DisplayMemberPath="Name" Margin="3" Height="25"></ListBox>
        <TextBlock Text="All faculty age" Margin="3"></TextBlock>
        <ListBox ItemsSource="{Binding}" DisplayMemberPath="Age" Margin="3" Height="25"></ListBox>
        <TextBlock Text="All faculty fav color" Margin="3"></TextBlock>
        <ListBox ItemsSource="{Binding}" DisplayMemberPath="FavColor" Margin="3" Height="25"></ListBox>
    </StackPanel>
</Window>
